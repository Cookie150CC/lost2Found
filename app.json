{
  "expo": {
    "name": "lost2Found",
    "description": "A very interesting project.",
    "slug": "lost2Found",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
        "supportsTablet": true,
          "bundleIdentifier": "com.idk.app",
          "config": {
            "googleMapsApiKey": "<API KEY HERE>"
          }
      },
      "android": {
          "package": "com.idk.app",
          "config": {
              "googleMaps": { "apiKey": "<API KEY HERE>" }
           }
      }
  }
}
